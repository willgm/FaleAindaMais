{
  "name": "FaleMais",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "bower install --allow-root --config.interactive=false",
    "prestart": "npm install --unsafe-perm",
    "start": "node cluster",
    "pretest": "npm install && webdriver-manager update",
    "test": "protractor test/protractor-conf.js",
    "predocker": "sudo docker run -d --name mongo mongo && sudo docker run -d --name redis redis && sudo docker build -t falemaisimg .",
    "docker": "sudo docker run -dp 5000:5000 -v ~/dev/FaleAindaMais/:/src --link mongo:mongo --link redis:redis --name falemais falemaisimg"
  },
  "dependencies": {
    "coffee-script": "1.7.1",
    "body-parser": "1.0.0",
    "cookie-parser": "1.0.1",
    "debug": "0.7.4",
    "express": "4.0.0",
    "express-load": "1.1.9",
    "express-session": "1.0.3",
    "jade": "1.3.0",
    "method-override": "1.0.0",
    "mongoose": "3.8.8",
    "morgan": "1.0.0",
    "static-favicon": "1.0.0",
    "redis": "0.10.1",
    "connect-redis": "2.0.0",
    "compression": "1.0.2",
    "bower": "1.3.12"
  },
  "devDependencies": {
    "protractor": "~0.20.1"
  }
}
